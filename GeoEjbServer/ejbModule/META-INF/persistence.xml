<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd">

	<persistence-unit name="geoJPA" transaction-type="JTA">

		<jta-data-source>jdbc/geonotesDS</jta-data-source>

		<class>org.geonotes.entities.Administrateur</class>
		<class>org.geonotes.entities.Note</class>
		<class>org.geonotes.entities.Parcour</class>
		<class>org.geonotes.entities.Utilisateur</class>
		<class>org.geonotes.entities.Categorie</class>
		<class>org.geonotes.entities.Users</class>
	</persistence-unit>

</persistence>